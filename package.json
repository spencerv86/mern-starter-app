{
  "name": "mern-starter-app",
  "version": "1.0.0",
  "description": "1. \"touch server.js\"\r 2. \"npm init-y\"\r 3. \"npm install express\"\r 4. \"npm install express mongoose\"\r 4",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencerv86/mern-starter-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spencerv86/mern-starter-app/issues"
  },
  "homepage": "https://github.com/spencerv86/mern-starter-app#readme",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.11.17"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
